{
  "name": "kirbyup",
  "version": "2.2.1",
  "packageManager": "pnpm@8.6.3",
  "description": "Zero-config bundler for Kirby Panel plugins",
  "author": {
    "name": "Johann Schopplich",
    "email": "pkg@johannschopplich.com",
    "url": "https://johannschopplich.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/johannschopplich/kirbyup#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johannschopplich/kirbyup.git"
  },
  "bugs": "https://github.com/johannschopplich/kirbyup/issues",
  "keywords": [
    "kirby-cms",
    "kirby-plugin",
    "kirby",
    "cli",
    "panel",
    "bundler"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.mjs"
    },
    "./plugin": {
      "types": "./dist/plugin.d.ts",
      "import": "./dist/plugin.mjs"
    }
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "config": [
        "./dist/config.d.ts"
      ],
      "plugin": [
        "./dist/plugin.d.ts"
      ]
    }
  },
  "bin": "./cli.mjs",
  "files": [
    "bin",
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "STUB=true unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp --commit --push --tag",
    "test": "vitest",
    "test:types": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@vitejs/plugin-vue2": "2.2.0",
    "@vitejs/plugin-vue2-jsx": "1.1.0",
    "@vue/compiler-sfc": "^2.7.14",
    "cac": "^6.7.14",
    "chokidar": "^3.5.3",
    "consola": "^2.15.3",
    "detect-package-manager": "^2.0.1",
    "magic-string": "^0.30.0",
    "pathe": "^1.1.1",
    "perfect-debounce": "^0.1.3",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.24",
    "postcss-dir-pseudo-class": "^7.0.2",
    "postcss-load-config": "^4.0.1",
    "postcss-logical": "^6.2.0",
    "rollup-plugin-external-globals": "^0.8.0",
    "sass": "^1.63.4",
    "unconfig": "^0.3.9",
    "vite": "^4.3.9",
    "vite-plugin-full-reload": "^1.0.5",
    "vue": "^2.7.14"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.6",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.16.18",
    "@types/prompts": "^2.4.4",
    "bumpp": "^9.1.1",
    "eslint": "^8.43.0",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.1",
    "simple-git-hooks": "^2.8.1",
    "typescript": "~5.0.4",
    "unbuild": "^1.2.1",
    "vitest": "^0.29.8"
  },
  "simple-git-hooks": {
    "commit-msg": "node scripts/verifyCommit.mjs $1"
  }
}
